---
import { Image } from "astro:assets";

interface Props {
  src: ImageMetadata;
  alt: string;
  width: number;
  height: number;
}

const props = Astro.props;
---

<Image
  src={props.src}
  alt={props.alt}
  width={props.width}
  height={props.height}
  style={{
    border: "1px solid black",
  }}
/>
